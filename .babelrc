{
  "presets": [
    "next/babel"
  ],
  "plugins": ["polished"],
  "env": {
    "development": {
      "plugins": [
        ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
      ]
    },
    "production": {
      "plugins": [
        ["styled-components", { "ssr": false, "displayName": false, "preprocess": false } ]
      ]
    }
  }
}