{
  "name": "masifunde-fe",
  "version": "1.0.0",
  "description": "Frontend for the Masifunde website",
  "main": "index.js",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "next start",
    "dev": "next dev",
    "build": "npm run build:production",
    "build:production": "npm run _build && npm run _robots:production",
    "build:preview": "npm run _build && npm run _robots:preview",
    "_build": "next build && next export --threads 1",
    "_robots:production": "cp robots.production.txt out/robots.txt",
    "_robots:preview": "cp robots.preview.txt out/robots.txt",
    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "lint-fix": "npm run lint -- --fix",
    "prettier-check": "prettier --list-different components i18n pages propTypes styling utils *.js",
    "prettier-fix": "prettier --write components i18n pages propTypes styling utils *.js",
    "serve": "netlify dev -d out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurice/masifunde-fe.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/futurice/masifunde-fe/issues"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "prettier --list-different"
    ]
  },
  "homepage": "https://github.com/futurice/masifunde-fe#readme",
  "dependencies": {
    "contentful": "^9.1.32",
    "final-form": "^4.20.7",
    "final-form-calculate": "^1.3.2",
    "i18n-react": "^0.7.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "^12.0.0",
    "nprogress": "^0.2.0",
    "nuka-carousel": "^5.1.1",
    "polished": "^4.2.2",
    "promise-memoize": "^1.2.1",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^17.0.0",
    "react-async-script": "^1.2.0",
    "react-dom": "^17.0.0",
    "react-final-form": "^6.5.9",
    "react-ga": "^3.3.0",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.0",
    "react-modal": "^3.3.2",
    "react-share": "^4.4.0",
    "reactstrap": "^8.8.0",
    "sitemap": "^7.1.1",
    "styled-components": "^5.3.6",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^13.0.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "netlify-cli": "^12.2.8",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2"
  }
}
