{
  "name": "masifunde-fe",
  "version": "0.0.1",
  "description": "Frontend for the Masifunde website",
  "main": "index.js",
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch server.js --watch routes.js server.js",
    "build": "next build",
    "build-static": "next build && next export && npm run copy",
    "copy": "cp robots.txt out",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.js .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --write 'src/**/*.{js,jsx,css,json}'",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurice/masifunde-fe.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/futurice/masifunde-fe/issues"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  },
  "homepage": "https://github.com/futurice/masifunde-fe#readme",
  "dependencies": {
    "contentful": "^5.0.5",
    "lodash": "^4.17.4",
    "next": "^4.1.4",
    "next-routes": "^1.1.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-async-script": "^0.9.1",
    "react-dom": "^16.1.1",
    "react-icons": "^2.2.7",
    "react-markdown": "^2.5.1",
    "reactstrap": "^5.0.0-alpha.4",
    "styled-components": "^2.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-styled-components": "^1.3.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.2"
  }
}
